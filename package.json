{
	"name": "collapsible-markdown-headers",
	"displayName": "collapsible-markdown-headers",
	"publisher": "Achal",
	"description": "automatically collapse existing headers or add new collapsible headers in a markdown file\\",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.collapse_header",
		"onCommand:extension.collapse_header_detect_endpoint"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.collapse_header",
				"title": "Collapse Markdown Header"
			},
			
			{
				"command": "extension.collapse_header_detect_endpoint",
				"title": "Collapse Markdown Section"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"vsce": "^1.74.0",
		"vscode": "^1.1.36"
	}
}
